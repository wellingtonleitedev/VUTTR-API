    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.10.1",
    "swagger-ui-express": "^4.1.2",
    "youch": "^2.0.10"
  },
